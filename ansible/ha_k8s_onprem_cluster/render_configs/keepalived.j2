vrrp_instance VI_1 {
        state {{ node_role }}
        interface {{ network_interface }}
        virtual_router_id 51
        priority {{ node_priority }}
        advert_int 1
        authentication {
              auth_type PASS
              auth_pass {{ keepalived_pass }}
        }
        virtual_ipaddress {
              {{ node_floating_ip_addr }}
        }
}
